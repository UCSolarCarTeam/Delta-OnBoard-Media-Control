cam_list	viewWebcam.py	/^cam_list = pygame.camera.list_cameras()$/;"	v
imagen	viewWebcam.py	/^   imagen = webcam.get_image()$/;"	v
screen	viewWebcam.py	/^screen = pygame.display.set_mode((xwidth,yheight),pygame.FULLSCREEN|pygame.HWSURFACE)$/;"	v
webcam	viewWebcam.py	/^webcam = pygame.camera.Camera(cam_list[0],(xwidth,yheight))$/;"	v
xwidth	viewWebcam.py	/^xwidth = 900    $/;"	v
yheight	viewWebcam.py	/^yheight = 506$/;"	v
